// task6.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "iostream"
#include "bitset"

using namespace std;

int _tmain(int argc, _TCHAR* argv[])
{
	unsigned int A;
	cin >> A;
	cout << bitset<32>(A).to_string() << endl;
	unsigned int i;
	cin >> i;
	A = A & ~(1 << i);
	cout << bitset<32>(A).to_string() << endl << A;
	system("pause");
	return 0;
}

