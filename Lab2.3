#include "stdafx.h"
#include "iostream"
#include "bitset"

using namespace std;


int _tmain(int argc, _TCHAR* argv[])
{
	unsigned int A;
	unsigned int i;
	cin >> A;
	cin >> i;
	cout << bitset<32>(A).to_string() << endl;
	unsigned int res = (A << (32 - i)) >> (32 - i);
	cout << bitset<32>(res).to_string() << endl;
	cout << res << endl;
	system("pause");
	return 0;
}

